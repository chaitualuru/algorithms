str1 = "hl"
str2 = "lh"

character_list = [0]*26
character_list2 = [0]*26

for i in range(len(str1)):
	pos = ord(str1[i]) - ord('a')
	character_list[pos] += 1

for i in range(len(str2)):
	pos2 = ord(str2[i]) - ord('a')
	character_list2[pos2] += 1
	
if character_list == character_list2:
	print True
else:
	print False